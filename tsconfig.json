{
  "extends": "@dxsixpc/configs/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "docs/": ["docs"],
      "docs/*": ["docs/*"],
      "@dxsixpc/generator": ["src"],
      "@dxsixpc/generator/*": ["src/*"]
    },
    "types": ["react", "@emotion/react"]
  },
  "include": [".dumi/**/*", ".dumirc.ts", "src/**/*", "docs/**/*", ".eslintrc.js"]
}
